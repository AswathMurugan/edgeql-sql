{
  "tableName": "default",
  "description": "Default fallback schema for tables without specific configuration",
  "version": "1.0",
  "fields": {
    "id": {
      "type": "INTEGER",
      "nullable": false,
      "primaryKey": true,
      "description": "Unique record identifier"
    },
    "name": {
      "type": "VARCHAR",
      "length": 255,
      "description": "Generic name field"
    },
    "description": {
      "type": "VARCHAR",
      "length": 1000,
      "description": "Generic description field"
    },
    "status": {
      "type": "VARCHAR",
      "length": 50,
      "defaultValue": "ACTIVE",
      "description": "Generic status field"
    },
    "isActive": {
      "type": "BOOLEAN",
      "defaultValue": true,
      "description": "Generic active flag"
    },
    "createdDate": {
      "type": "TIMESTAMP",
      "nullable": false,
      "description": "Record creation timestamp"
    },
    "updatedDate": {
      "type": "TIMESTAMP",
      "description": "Record last update timestamp"
    },
    "version": {
      "type": "INTEGER",
      "defaultValue": 1,
      "description": "Record version for optimistic locking"
    }
  },
  "indexes": [
    {
      "name": "idx_name",
      "fields": ["name"]
    },
    {
      "name": "idx_status",
      "fields": ["status", "isActive"]
    },
    {
      "name": "idx_created_date",
      "fields": ["createdDate"]
    }
  ]
}